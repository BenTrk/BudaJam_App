<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.budajam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".classes.FirebaseHandler"
        android:allowBackup="true"
        android:icon="@mipmap/kisgeri24_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kisgeri24_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OptionsActivity">
        </activity>
        <activity android:name=".views.LoginActivity"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".views.SignupActivity">
        </activity>
        <activity android:name=".views.ResetPasswordActivity">
        </activity>
        <activity android:name=".views.CheckOutActivity">
        </activity>
        <activity android:name=".ExtraPointsActivity" />
        <activity android:name=".models.LoginModel" />

    </application>

</manifest>